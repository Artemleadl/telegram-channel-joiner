{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 60,
        "memory": 1024
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/bot.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "PYTHONASYNCIODEBUG": "1"
  }
} 